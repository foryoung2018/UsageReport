<resources>
    <!-- The set of attributes that describe a AlertDialog's theme. -->
    <declare-styleable name="AlertDialog">
        <attr name="android:fullDark" />
        <attr name="android:topDark" />
        <attr name="android:centerDark" />
        <attr name="android:bottomDark" />
        <attr name="android:fullBright" />
        <attr name="android:topBright" />
        <attr name="android:centerBright" />
        <attr name="android:bottomBright" />
        <attr name="android:bottomMedium" />
        <attr name="android:centerMedium" />
        <attr name="android:actionMenuTextColor" />
    </declare-styleable>
    <declare-styleable name="HtcFakescreen">
        <attr name="fakescreenDisable" format="boolean" />
        <!-- Mark.SL_Chen for starting window layout[ -->
        <attr name="android:initialLayout" />
        <!-- Mark.SL_Chen for starting window layout] -->
    </declare-styleable>
    <!-- add for sense60 start -->
    <declare-styleable name="HtcListItemSerialNumber">
        <attr name="android:background" />
    </declare-styleable>
    <declare-styleable name="HtcDivider">
        <attr name="android:dividerHorizontal" />
        <attr name="android:dividerVertical" />
    </declare-styleable>
    <declare-styleable name="HtcEmptyView">
        <attr name="android:text" />
    </declare-styleable>
    <!-- add for sense60 end -->
    <attr name="displayMode">
        <enum name="mode_light" value="0x00" />
        <enum name="mode_dark" value="0x01" />
        <enum name="mode_full" value="0x02" />
    </attr>
</resources>