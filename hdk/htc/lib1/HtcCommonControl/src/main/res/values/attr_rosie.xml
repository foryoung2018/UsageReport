<resources>
    <!-- Added by John Lin for Rosie 3.0 widgets -->
    <declare-styleable name="HtcRosieWidget">
        <!-- Identity/Name of the widget. -->
        <attr name="id" format="string" />
        <!-- Information to be displayed in picker UI. -->
        <!-- Display Name of the widget. -->
        <attr name="label" format="reference|string" />
        <!--
            Release date string.
            TBD: should it be formatted by user setting?
        -->
        <attr name="date" format="reference" />
    </declare-styleable>
</resources>
