<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Račun HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Povezave ni mogoče vzpostaviti</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Za uporabo te aplikacije potrebujete omrežno povezavo. Vzpostavite mobilno podatkovno ali brezžično povezavo.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Prosimo, počakajte ...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Povezave s strežniki HTC ni mogoče vzpostaviti. Poskusite znova pozneje.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Za uporabo te aplikacije potrebujete omrežno povezavo.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Ustvarite Googlov račun, s katerim se boste prijavili.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Povezave ni mogoče vzpostaviti. Poskusite znova.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Do Googlovega računa ne moremo dostopati. Preverite, ali je račun aktiven.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Zahtevaj dovoljenja</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Če se želite prijaviti z Googlovim računom, mora ta aplikacija dostopiti do vašega računa. Ob pozivu dodelite dovoljenje.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Ta aplikacija mora za pravilno delovanje dostopati do vaših računov. Dovoljenja za %1$s lahko upravljate v Nastavitvah.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Ustvarite račun HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-poštni naslov</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Račun HTC z vašim e-poštnim naslovom je bil že ustvarjen. Za prijavo tapnite račun ali ustvarite novega.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Pošiljajte mi HTC-jeve ponudbe, informacije o izdelkih in novice.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Uporaba elektronskega naslova iz računa %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Ustvari račun</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Ustvari nov račun</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Z ustvarjanjem računa soglašate s %1$s za račun HTC in potrjujete %2$s. Za vpis morate imeti vsaj 14 let.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Pogoji</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Pravilnik o zasebnosti</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Pravilnik o zasebnosti</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Pogoji</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Preverite svoj račun</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Odpri e-poštno sporočilo</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Ne zdaj</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Za prijavo v račun HTC morate s povezavo, poslano na %1$s, preveriti svoj e-poštni naslov.\n\nČe niste prejeli e-poštnega sporočila, preverite mapo z neželeno pošto ali %2$s.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">pošlji znova</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Potreben je elektronski naslov.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Vnesite veljaven e-poštni naslov.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-poštno sporočilo je bilo poslano za: %s</string><!--Email sent to: %s-->

