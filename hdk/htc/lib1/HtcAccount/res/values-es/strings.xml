<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Cuenta de HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Incapaz de conectar</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Necesitas una conexión de red para utilizar esta aplicación. Conectar con los datos móviles o con la red WiFi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Por favor, espera...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">No podemos conectar a los servidores de HTC. Inténtalo de nuevo más tarde.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Necesitas una conexión de red para utilizar esta aplicación.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Crea una cuenta de Google con la que iniciar sesión.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">No se ha podido conectar. Vuelve a intentarlo más tarde.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">No podemos acceder a tu cuenta de Google. Comprueba que la cuenta está activa.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Solicitar permisos</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Para iniciar sesión con la cuenta de Google, esta aplicación necesita acceder a tu cuenta. Permitir cuando se te pregunte.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Para que funcione correctamente, esta aplicación necesita acceder a tus cuentas. Puedes administrar los permisos de %1$s en Ajustes.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Crear una cuenta de HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Dirección de correo electrónico</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Ya se ha creado una cuenta de HTC con tu dirección de correo electrónico. Pulsa en una cuenta para iniciar sesión o crear una cuenta nueva.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Enviarme por correo electrónico ofertas, información del productos y boletines de noticias de HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Uso del correo electrónico de la cuenta %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Crear cuenta</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Crear una cuenta nueva</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Al crear una cuenta, estás de acuerdo con %1$s de la cuenta de HTC y reconoces a %2$s. Debes tener al menos 14 años de edad para registrarte.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Términos y condiciones</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Política de privacidad</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Política de privacidad</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Términos y condiciones</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Verifica tu cuenta</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Abrir correo electrónico</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">No ahora</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Para iniciar sesión en la cuenta de HTC, es necesario que verifiques tu dirección de correo electrónico utilizando el enlace enviado a %1$s.\n\nSi no has recibido el correo electrónico, comprueba tu carpeta de correo no deseado o %2$s ahora.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">volver a enviar</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Se requiere una dirección de correo electrónico.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Introduce una dirección de correo electrónico válida.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">Correo electrónico enviado a: %s</string><!--Email sent to: %s-->

