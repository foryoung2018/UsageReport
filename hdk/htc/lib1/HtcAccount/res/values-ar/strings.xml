    <string name="txt_social_email_tips">استخدام بريد إلكتروني من حساب %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">إنشاء حساب</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">إنشاء حساب جديد</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">إن إنشاءك لحساب يعني موافقتك على %1$s بشأن حساب HTC وإقرارك بـ%2$s. يجب ألا يقل سنك عن 14 عامًا للاشتراك.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">البنود والشروط</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">سياسة الخصوصية</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">سياسة الخصوصية</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">البنود والشروط</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">إثبات ملكيتك للحساب</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">فتح البريد</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">ليس الآن</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">لتسجيل الدخول إلى حساب HTC، يجب التحقق من صحة عنوان بريدك الإلكتروني باستخدام الرابط الذي تم إرساله إلى %1$s.\n\nإذا لم تتلقَ الرسالة الإلكترونية، فابحث في مجلد الرسائل غير المرغوب فيها أو %2$s الآن.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">إعادة إرسال</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">مطلوب عنوان بريد إلكتروني.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">الرجاء إدخال عنوان بريد إلكتروني صالح.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">تم إرسال رسالة إلكترونية إلى: %s</string><!--Email sent to: %s-->

