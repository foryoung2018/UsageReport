<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Konto HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Pripojenie nie je možné</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Pri používaní tejto aplikácie je potrebné pripojenie k sieti. Pripojte k mobilnej dátovej sieti alebo Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Čakajte, prosím...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Nedá sa pripojiť k serverom HTC. Skúste to znova neskôr.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Pri používaní tejto aplikácie je potrebné pripojenie k sieti.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Vytvorte si konto Google, ak sa pomocou neho chcete prihlásiť.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Pripojenie nie je možné. Skúste to znova.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Prístup ku kontu Google nebol možný. Skontrolujte, či je konto aktívne.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Vyžiadať povolenia</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Ak sa chcete prihlásiť cez konto Google, aplikácia potrebuje prístup k vášmu kontu. Keď sa objaví výzva, povoľte.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Na správne fungovanie potrebuje táto aplikácia prístup k vašim kontám. Povolenia aplikácie %1$s môžete spravovať cez Nastavenie.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Vytvoriť konto HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-mailová adresa</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Konto HTC s vašou e-mailovou adresou už bolo vytvorené. Klepnutím na konto sa prihláste, prípadne si vytvorte nové konto.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Posielať mi e-mailom ponuky od HTC, informácie o produktoch a novinky.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Pomocou e-mailu z konta: %s</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Vytvoriť konto</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Vytvoriť nové konto</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Vytvorením konta súhlasíte s dokumentom %1$s ku Kontu HTC a beriete na vedomie %2$s. Registrácia je od 14 rokov.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Zmluvné podmienky</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Zásady ochrany súkromia</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Zásady ochrany súkromia</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Zmluvné podmienky</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Overte svoje konto</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Otvoriť e-mail</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Teraz nie</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Ak sa chcete prihlásiť na svoje Konto HTC, je potrebné overiť e-mailovú adresu pomocou prepojenia, ktoré bolo odoslané na adresu %1$s.\n\nAk ste e-mail nedostali, skontrolujte priečinok nevyžiadanej pošty, prípadne teraz %2$s.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">poslať znova</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">E-mailová adresa je povinný údaj.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Zadajte platnú e-mailovú adresu.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-mail odoslaný na: %s</string><!--Email sent to: %s-->

