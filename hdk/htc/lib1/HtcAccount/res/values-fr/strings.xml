<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Compte HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Connexion impossible</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Une connexion réseau est nécessaire pour utiliser cette application. Veuillez vous connecter aux données mobiles ou à Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Veuillez patienter...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Nous ne pouvons pas connecter aux serveurs HTC. Veuillez réessayer plus tard.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Une connexion réseau est nécessaire pour utiliser cette application.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Veuillez créer un compte Google avec lequel vous connecter.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Connexion impossible. Veuillez réessayer.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Nous ne pouvons pas accéder à votre compte Google. Veuillez vous assurer que le compte est actif.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Nécessite des autorisations</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Pour se connecter avec le compte Google, cette appli a besoin d\'accéder à votre compte. Veuillez accorder l\'autorisation quand demandée.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Pour fonctionner correctement, cette appli a besoin d\'accéder à vos comptes. Vous pouvez gérer les autorisations de %1$s dans les Paramètres.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Créer un compte HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Adresse e-mail</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Un compte HTC avec votre adresse e-mail a déjà été créé. Appuyez sur un compte pour vous connecter ou créez un nouveau compte.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Envoyez-moi des offres, infos produits et newsletters de HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Utilise l\'e-mail du compte %s :</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Créer le compte</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Créer un nouveau compte</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">En créant un compte, vous acceptez les %1$s du compte HTC et reconnaissez la %2$s. Vous devez avoir au moins 14 ans pour vous inscrire.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Conditions d\'utilisation</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Politique de vie privée</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Politique de vie privée</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Conditions d\'utilisation</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Vérifiez votre compte</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Ouvrez l\'email</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Pas maintenant</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Pour vous connecter au compte de HTC, vous devez vérifier votre adresse e-mail en utilisant le lien envoyé à %1$s.\n\nSi vous n\'avez pas reçu l\'e-mail, vérifiez votre dossier de spams ou %2$s le maintenant.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">renvoyez</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Adresse e-mail nécessaire.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Veuillez entrer une adresse e-mail valide.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-mail envoyé à : %s</string><!--Email sent to: %s-->

<!-- ******************************************************************************** Common string replacements. ******************************************************************************** -->
    <eat-comment />

<!-- Maps to common string "nn_settings" -->
    <string name="nn_settings">Paramètres</string><!--Settings-->

<!-- Maps to common string "va_close" -->
    <string name="va_close">Fermer</string><!--Close-->

<!-- Maps to common string "va_back" -->
    <string name="va_back">Retour</string><!--Back-->

<!-- Maps to common string "va_cancel" -->
    <string name="va_cancel">Annuler</string><!--Cancel-->

</resources>
