<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Akaun HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Tidak dapat menyambung</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Anda memerlukan sambungan rangkaian untuk menggunakan aplikasi ini. Sila sambung ke data mudah alih atau Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Sila tunggu...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Kami tidak dapat menyambung ke pelayan HTC. Sila cuba semula kemudian.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Anda memerlukan sambungan rangkaian untuk menggunakan aplikasi ini.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Sila cipta Akaun Google untuk mendaftar masuk.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Tidak dapat menyambung. Sila cuba lagi.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Kami tidak dapat mengakses Akaun Google anda. Sila pastikan akaun aktif.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Perlukan kebenaran</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Untuk mendaftar masuk dengan Akaun Google, aplikasi ini perlu mengakses akaun anda. Sila berikan kebenaran apabila diminta.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">\"Untuk berfungsi dengan betul, aplikasi ini perlu mengakses akaun anda. Anda boleh mengurus kebenaran %1$s dalam Tetapan.\"</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Cipta Akaun HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Alamat e-mel</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Sebuah Akaun HTC dengan alamat e-mel anda sudah dicipta. Ketik akaun untuk mendaftar masuk atau cipta akaun baharu.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">E-melkan kepada saya tawaran, maklumat produk dan surat berita daripada HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Menggunakan e-mel daripada akaun %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Buat akaun</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Cipta akaun baharu</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Dengan mencipta akaun, anda bersetuju dengan %1$s Akaun HTC dan mengakui %2$s. Anda mesti sekurang-kurangnya berumur 14 tahun untuk mendaftar.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Terma dan syarat</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Dasar Privasi</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Dasar privasi</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Terma dan Syarat</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Sahkan akaun anda</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Buka mel</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Bukan sekarang</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Untuk mendaftar masuk ke Akaun HTC, anda perlu mengesahkan alamat e-mel anda menggunakan pautan yang dihantar ke %1$s.\n\nJika anda tidak mendapat e-mel tersebut, semak folder spam anda atau %2$snya sekarang.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">hantar semula</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Alamat e-mel diperlukan.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Sila masukkan alamat e-mel yang sah.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-mel dihantar ke: %s</string><!--Email sent to: %s-->

