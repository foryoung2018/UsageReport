<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC-konto</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Kunne ikke oprette forbindelse</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Du skal bruge en netværksforbindelse for at bruge dette program. Opret forbindelse til mobildate- eller Wi-Fi-forbindelse.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Vent venligst…</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Vi kan ikke oprette forbindelse til HTC-serverne. Prøv igen senere.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Du skal have en internetforbindelse, før du kan bruge dette program.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Opret en Google-konto til at logge på med.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Kunne ikke oprette forbindelse. Prøv igen.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Vi får ikke adgang til Google-kontoen. Kontroller, at kontoen er aktiv.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Kræver tilladelser</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">For at logge på med en Google-konto, skal dette program have adgang dine konti. Giv tilladelse, når du bliver bedt om det.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">\"For at kunne fungere korrekt, skal dette program have adgang dine konti. Du kan administrere tilladelser til %1$s i Indstillinger.\"</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Opret en HTC-konto</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-mail-adresse</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Der er allerede oprettet en HTC-konto med din e-mailadresse. Tryk på en konto for at logge på, eller opret en ny konto.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">E-mail mig tilbud, produktinfo og nyhedsbreve fra HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Sådan bruger du e-mail fra %s-konto:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Opret konto</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Opret ny konto</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Når du opretter en konto, accepterer du HTC-kontoens %1$s samt %2$s. Du skal være mindst 14 år gammel for at tilmelde dig.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Vilkår og betingelser</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Privatlivspolitik</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Privatlivspolitik</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Vilkår og betingelser</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Bekræft din konto</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Åbn e-mail</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Ikke nu</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">For at logge på HTC-konto, skal du bekræfte din e-mailadresse ved at bruge linket, sendt til %1$s.\n\nHvis du ikke har modtaget e-mailen, kan du tjekke din spammappe eller %2$s det nu.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">send igen</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">E-mail-adresse kræves.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Indtast en gyldig e-mail-adresse.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-mail sendt til: %s</string><!--Email sent to: %s-->

<!-- ******************************************************************************** Common string replacements. ******************************************************************************** -->
    <eat-comment />

<!-- Maps to common string "nn_settings" -->
    <string name="nn_settings">Indstillinger</string><!--Settings-->

<!-- Maps to common string "va_close" -->
    <string name="va_close">Luk</string><!--Close-->

<!-- Maps to common string "va_back" -->
    <string name="va_back">Tilbage</string><!--Back-->

<!-- Maps to common string "va_cancel" -->
    <string name="va_cancel">Annuller</string><!--Cancel-->

</resources>
