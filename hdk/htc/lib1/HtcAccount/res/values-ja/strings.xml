<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC アカウント</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">接続できません</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">このアプリケーションをご使用になるには、ネットワーク接続が必要です。モバイル データまたは Wi-Fi に接続してください。</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">お待ちください...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">HTC サーバーに接続できません。後でやり直してください。</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">このアプリケーションを使用するには、ネットワーク接続が必要です。</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">サインインするため、Google アカウントを作成してください。</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">接続できません。もう一度やり直してください。</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Google アカウントにアクセスできません。アカウントがアクティブになっているか確認してください。</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">権限が必要です</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">このアプリが Google アカウントにサインインするためには、アカウントにアクセスする必要があります。メッセージが表示されたら許可してください。</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">\"このアプリが正常に機能するためには、アカウントにアクセスする必要があります。%1$s の許可は設定内で管理できます。\" </string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">HTC アカウントを作成</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">電子メールアドレス</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">この電子メールアドレスの HTC アカウントは既に作成済みです。アカウントをタップしてサインインするか、新しいアカウントを作成してください。</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">HTC オファー、製品情報、ニュースレターを電子メールでお届けします。</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">%s アカウントから電子メールを使う：</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">新しいアカウントを作成する</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">新しいアカウントを作成</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">アカウントを作成することで、HTC アカウントの%1$sに同意し、%2$sについて理解されているものとします。サインアップするには14歳以上である必要があります。</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">利用規約</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">プライバシーポリシー</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">プライバシーポリシー </string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">利用規約</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">アカウントを認証</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">メールを開く</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">後で行う</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">HTC アカウントにサインインするには、%1$s に送信されたリンクを使って電子メールアドレスを確認する必要があります。\n\n電子メールを受け取っていない場合は、迷惑メールフォルダを確認するか、今すぐ %2$s してください。</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">再送信</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">電子メールアドレスが必要です。</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">有効な電子メールアドレスを入力してください。</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">電子メール送信先：%s</string><!--Email sent to: %s-->

