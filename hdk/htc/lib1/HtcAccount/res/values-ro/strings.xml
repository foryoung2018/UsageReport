    <string name="va_close">Închidere</string><!--Close-->

<!-- Maps to common string "va_back" -->
    <string name="va_back">Înapoi</string><!--Back-->

<!-- Maps to common string "va_cancel" -->
    <string name="va_cancel">Anulare</string><!--Cancel-->

</resources>
