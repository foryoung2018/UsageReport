<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Cont HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Conectare imposibilă</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Aveţi nevoie de o conexiune la reţea pentru a utiliza această aplicaţie. Conectaţi-vă la conexiunea de date mobile sau Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Aşteptaţi...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Conectare imposibilă la serverele HTC. Vă rugăm să încercaţi din nou mai târziu.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Aveţi nevoie de o conexiune la reţea pentru a utiliza această aplicaţie.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Creați un cont Google cu care să vă conectați.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Conectare imposibilă. Încercaţi din nou.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Nu vă putem accesa contul Google. Asigurați-vă că acest cont este activ.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Solicitare permisiuni</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Pentru a vă conecta cu contul Google, această aplicație are nevoie de acces la contul dvs. Acordaţi permisiunea când vă este solicitată.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Pentru a funcționa corespunzător, această aplicație are nevoie de acces la conturile dvs. Puteţi gestiona permisiunile %1$s din Setări.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Creare cont HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Adresă e-mail</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Există deja creat un Cont HTC cu adresa dvs. de e-mail. Apăsați un cont pentru autentificare, sau creați un cont nou.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Trimite-mi oferte, informații despre produs și buletine informative de la HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Utilizând e-mail din contul %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Creare cont</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Creați un cont nou</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Creând contul, sunteți de acord cu %1$s pentru Contul HTC și confirmați %2$s. Pentru abonare, trebuie să aveți minim 14 ani.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Termeni şi condiţii</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Politica de confidenţialitate</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Politica de confidenţialitate</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Termeni şi condiţii</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Confirmați-vă contul</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Deschideți e-mailul</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Nu acum</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Pentru autentificarea în Contul HTC, trebuie să vă confirmați adresa de e-mail utilizând linkul trimis către %1$s.\n\nDacă nu ați primit e-mailul, verificați-vă folderul de spam sau %2$s acum.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">retrimitere</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Adresa e-mail este obligatorie.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Introduceţi o adresă de e-mail corectă.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">A fost trimis un e-mail la: %s</string><!--Email sent to: %s-->

<!-- ******************************************************************************** Common string replacements. ******************************************************************************** -->
    <eat-comment />

