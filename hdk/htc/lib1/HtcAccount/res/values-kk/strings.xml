<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC тіркелгісі</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Қосылу мүмкін емес</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Бұл қолданбаны пайдалану үшін, желі байланысы қажет. Ұялы деректерге немесе Wi-Fi желісіне қосылыңыз.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Күте тұрыңыз...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">HTC серверлеріне қосылу мүмкін емес. Әрекетті кейін қайталаңыз.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Бұл қолданбаны пайдалану үшін желілік байланыс қажет.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Жүйеге кіру үшін Google тіркелгісін жасаңыз.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Қосылу мүмкін емес. Әрекетті қайталаңыз.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Google тіркелгіңізге кіру мүмкін емес. Тіркелгіңіз белсенді екеніне көз жеткізіңіз.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Рұқсаттар талап ету</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Google тіркелгісімен кіру үшін бұл қолданба тіркелгіңізге кіруі тиіс. Сұралған кезде рұқсат беріңіз.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Дұрыс жұмыс істеуі үшін бұл қолданба тіркелгілеріңізге кіруі тиіс. %1$s рұқсаттарын Параметрлер бойынша басқаруға болады.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">HTC тіркелгісін жасау</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Электрондық пошта мекенжайы</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Электрондық пошта мекенжайыңызбен HTC тіркелгісі бұрыннан жасалған. Кіру үшін тіркелгіні түртіңіз немесе жаңа тіркелгі жасаңыз.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Маған HTC ұсыныстарын, өнім туралы ақпаратын және жаршыларын электрондық поштамен жіберіңіз.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">%s тіркелгісінен электрондық пошта пайдалану:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Тіркелгі жасау</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Жаңа тіркелгі жасау</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Тіркелгі жасау арқылы HTC тіркелгісінің %1$s келісесіз және %2$s мақұлдайсыз. Тіркелу үшін жасыңыз 14 не одан жоғары болуы керек.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Шарттар мен ережелер</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Құпиялық саясаты</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Құпиялылық саясаты</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Шарттар мен ережелер</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Тіркелгіңізді тексеріңіз</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Поштаны ашу</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Қазір емес</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">HTC тіркелгісіне кіру үшін электрондық пошта мекенжайыңызды %1$s поштасына жіберілген сілтеменің көмегімен растау керек.\n\nЕгер электрондық пошта алмасаңыз, спам қалтасын тексеріңіз немесе оны %2$s тексеріңіз.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">қайта жіберу</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Электрондық пошта мекенжайы қажет.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Жарамды электрондық пошта мекенжайын енгізіңіз.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">Электрондық пошта мында жіберілген: %s</string><!--Email sent to: %s-->

