<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC konto</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Ühendust ei saa luua</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Selle rakenduse kasutamiseks vajate võrguühendust. Looge ühendus mobiilse andmeside või WiFiga.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Palun oodake...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Me ei saa HTC serveritega ühendust. Proovige hiljem uuesti.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Selle rakenduse kasutamiseks vajate võrguühendust.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Looge Google\'i konto, millega sisse logida.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Ühendust ei saa luua. Proovige uuesti.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Me ei pääse teie Google\'i kontole ligi. Veenduge, et konto oleks aktiivne.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Nõua luba</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Google\'i kontoga sisselogimiseks vajab see rakendus juurdepääsu teie kontole. Andke luba, kui seda küsitakse.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Õigesti töötamiseks vajab rakendus juurdepääsu teie kontodele. Saate hallata rakenduse %1$s lubasid seadetes.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Loo HTC konto</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-posti aadress</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Teie e-posti aadressiga on juba loodud HTC konto. Toksake kontot, et sisse logida, või looge uus konto.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Saada mulle e-posti teel HTC pakkumisi, tooteteavet ja uudiskirju.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">%s konto e-posti kasutamine:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Loo konto</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Loo uus konto</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Konto loomisel nõustute HTC konto dokumendiga %1$s ja võtate teadmiseks dokumendi %2$s. Registreerumiseks peate olema vähemalt 14-aastane.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Tingimused</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Privaatsuspoliitika</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Privaatsuspoliitika</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Tingimused</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Konto kinnitamine</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Ava e-post</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Mitte praegu</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">HTC kontosse sisselogimiseks peate oma e-posti aadressi kinnitama, kasutades e-posti aadressile %1$s saadetud linki.\n\nKui te ei saanud e-kirja kätte, kontrollige rämpspostikausta või %2$s see kohe.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">saada uuesti</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Nõutakse e-posti aadressi.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Sisestage kehtiv e-posti aadress.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-kiri saadeti aadressile %s</string><!--Email sent to: %s-->

