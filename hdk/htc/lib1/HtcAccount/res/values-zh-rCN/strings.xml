    <string name="toast_txt_provide_email">需要提供电子邮件地址。</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">请输入有效的电子邮件地址。</string><!--Please enter a valid email address.-->

