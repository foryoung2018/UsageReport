<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">使用此应用程序需要网络连接。</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">请创建登录使用的 Google 账户。</string><!--Please create a Google Account to sign in with.-->

