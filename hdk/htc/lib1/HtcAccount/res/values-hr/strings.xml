<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC račun</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Povezivanje nije moguće</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Za uporabu ove aplikacije potrebno je povezivanje s mrežom. Uključite mobilnu podatkovnu mrežu ili Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Pričekajte...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Povezivanje s HTC poslužiteljima nije uspjelo. Pokušajte ponovno kasnije.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Za uporabu ove aplikacije potrebno je povezivanje s mrežom.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Načinite Google račun s kojim se želite prijaviti.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Povezivanje nije moguće. Pokušajte ponovno.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Ne možemo pristupiti vašem Google računu. Provjerite je li račun aktivan.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Potrebne su dozvole</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Za prijavu Google računom ova aplikacija treba pristup vašem računu. Nakon upita dajte dozvolu.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Za pravilan rad ova aplikacija treba pristup vašim računima. U postavkama možete upravljati dozvolama za %1$s.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Načinite HTC račun</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">Adresa e-pošte</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">HTC račun s vašom adresom e-pošte je već kreiran. Dodirnite račun radi prijave ili kreirajte novi račun.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">E-poštom mi šalji ponude, informacije o proizvodima i novosti iz HTC-a.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Korištenje e-pošte %s računa:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Načinite račun</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Načini novi račun</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Stvaranjem računa pristajete na %1$s HTC računa i potvrđujete %2$s. Trebate biti stariji od 14 godina da biste se mogli prijaviti.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Uvjeti i odredbe</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Politika privatnosti</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Pravila privatnosti</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Uvjeti i odredbe</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Potvrdite svoj račun</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Otvorite e-poštu</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Ne sada</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Za prijavu u HTC račun trebate potvrditi svoju adresu e-pošte poveznicom poslanom na %1$s.\n\nAko niste dobili e-poštu provjerite mapu s neželjenom poštom ili %2$s odmah.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">pošalji ponovno</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Potrebna je adresa e-pošte.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Unesite ispravnu adresu e-pošte.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-pošta poslana na: %s</string><!--Email sent to: %s-->

