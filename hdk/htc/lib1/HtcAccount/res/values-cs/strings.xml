<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">Účet HTC</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Připojení se nezdařilo</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Pro používání této aplikace potřebujete síťové připojení. Připojte se k mobilnímu datovému připojení nebo Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Počkejte prosím...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Připojení k serverům HTC není možné. Opakujte akci později.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Pro používání této aplikace potřebujete síťové připojení.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Prosím, vytvořte si účet Google, s nímž se budete přihlašovat.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Nelze se připojit. Opakujte akci.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Nelze získat přístup k vašemu účtu Google. Přesvědčte se prosím, zda je účet aktivní.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Vyžadovat oprávnění</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Pro přihlášení k účtu Google tato aplikace vyžaduje přístup k vašim účtům. Na vyžádání umožněte povolení.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">\"Pro řádnou funkci tato aplikace vyžaduje přístup k vašim účtům. Povolení %1$s můžete spravovat v Nastaveních.\"</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Vytvořit účet HTC</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-mailová adresa</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Účet HTC s vaší e-mailovou adresou byl již vytvořen. Klepněte na účet pro přihlášení nebo vytvořte nový účet.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Posílejte mi e-mailem nabídky, informace o produktech a newslettry společnosti HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Používání e-mailu od účtu %s:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Vytvořit účet</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Vytvořit nový účet</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Vytvořením účtu vyjadřujete svůj souhlas se %1$s účtu HTC a přijímáte %2$s. Pro registraci musíte být starší 14 let.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Smluvní podmínky</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Zásady ochrany osobních údajů</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Zásady ochrany osobních údajů</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Smluvní podmínky</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Ověřit váš účet</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Otevřít mail</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Nyní ne</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Pro přihlášení k účtu HTC musíte ověřit svou e-mailovou adresu pomocí odkazu odeslaného na %1$s.\n\nPokud jste e-mail nedostali, podívejte se do složky pro nevyžádanou poštu nebo %2$s nyní.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">odeslat znovu</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">E-mailová adresa je povinná.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Zadejte platnou e-mailovou adresu.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-mail byl odeslán na adresu: %s</string><!--Email sent to: %s-->

