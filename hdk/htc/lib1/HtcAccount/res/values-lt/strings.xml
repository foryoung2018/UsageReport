<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC paskyra</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Nepavyksta prisijungti</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">Jums reikia tinklo ryšio, kad galėtumėte naudoti šią programą. Prisijunkite prie mobiliojo duomenų perdavimo ryšio arba „Wi-Fi“ ryšio.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Prašome palaukti...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Nepavyksta prisijungti prie HTC serverių. Bandykite dar kartą vėliau.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">Jums reikia tinklo ryšio, kad galėtumėte naudoti šią programą.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Sukurkite „Google“ paskyrą, su kuria galėsite prisijungti.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Nepavyksta prisijungti. Bandykite dar kartą.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Mums nepavyko pasiekti jūsų „Google“ paskyros. Pasirūpinkite, kad paskyra būtų aktyvi.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Reikalauti leidimų</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">Norint prisijungti naudojant „Google“ paskyrą, šiai programėlei reikia prieigos prie jūsų paskyros. Gavę užklausą duokite leidimą.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">&quot;Kad ši programėlė veiktų tinkamai, ji turi pasiekti jūsų paskyras. %1$s leidimus galima valdyti Nuostatose.&quot;</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Sukurkite HTC paskyrą</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">El. pašto adresas</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">HTC paskyra jūsų el. pašto adresu jau sukurta. Palieskite paskyrą, kad prisijungtumėte, arba sukurkite naują paskyrą.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Siųskite man el. paštu HTC pasiūlymus, produktų informaciją ir informacinius biuletenius.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Naudojamas paskyros %s el. paštas:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Sukurti paskyrą</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Sukurti naują paskyrą</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Sukurdami paskyrą sutinkate su HTC paskyros dokumentu %1$s ir patvirtinate dokumentą %2$s. Norėdami užsiregistruoti, turite būtų bent 14 metų amžiaus.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Sąlygos ir nuostatai</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Privatumo politika</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Privatumo politika</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Sąlygos ir nuostatai</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Patvirtinti savo paskyrą</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Atverti el. laišką</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Ne dabar</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">Norėdami prisijungti prie HTC paskyros, turėsite patvirtinti savo el. pašto adresą, naudodamiesi saitu, nusiųstu gavėjui %1$s.\n\nJei el. laiško negavote, patikrinkite savo brukalo aplanką arba %2$s jau dabar.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">siųsti iš naujo</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">Reikia el. pašto adreso.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Įveskite galiojantįjį el. pašto adresą.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">Kam išsiųstas el. laiškas: %s</string><!--Email sent to: %s-->

