<?xml version="1.0" encoding="utf-8"?>
<resources>

<!-- Application label shows in Account & Sync. -->
    <string name="label_htc_account">HTC-konto</string><!--HTC Account-->

<!-- Network unavailable dialog. Title. -->
    <string name="dialog_title_network_unavailable">Kan ej ansluta</string><!--Unable to connect-->

<!-- Network unavailable dialog. Message. -->
    <string name="dialog_msg_network_unavailable">En nätverksanslutning krävs för att använda det här programmet. Anslut till mobil data- eller Wi-Fi.</string><!--You need a network connection to use this application. Please connect to mobile data or Wi-/Fi.-->

<!-- Progress dialog message to show when working. -->
    <string name="dialog_msg_please_wait">Vänta...</string><!--Please wait…-->

<!-- General sign-in/sign-up error. -->
    <string name="toast_txt_error_general_failure">Vi kan inte ansluta till HTC:s servrar. Försök igen senare.</string><!--We are unable to connect to the HTC servers. Please try again later.-->

<!-- Data network unavailable -->
    <string name="toast_txt_error_network_unavailable">En nätverksanslutning krävs för att använda det här programmet.</string><!--You need a network connection to use this application.-->

<!-- Unable to access Google account. -->
    <string name="toast_txt_no_google_account">Skapa ett Google-konto att logga in med.</string><!--Please create a Google Account to sign in with.-->

<!-- Accessing integrated social networks failed. -->
    <string name="toast_txt_error_access_social_networks_failed">Det går inte att ansluta. Försök igen.</string><!--Unable to connect. Please try again.-->

<!-- General Google account sign-in/sign-up error. -->
    <string name="toast_txt_error_signing_in_google_account">Det går inte att komma åt ditt Google-konto. Kontrollera att kontot är aktiverat.</string><!--We are unable to access your Google Account. Please make sure the account is active.-->

<!-- Request permission dialog title -->
    <string name="dialog_msg_account_permission_title">Kräver behörigheter</string><!--Require permissions-->

<!-- Request permission dialog message -->
    <string name="dialog_msg_account_permission_rationale">För att logga in med Google-kontot måste detta program få åtkomst till ditt konto. Ge behörighet när du tillfrågas.</string><!--To sign in with Google Account, this app needs to access your account. Please allow permission when asked.-->

<!-- Request permission dialog message, which direct user to app detailed Settings -->
    <string name="dialog_msg_account_permission_settings">\"För att fungera korrekt måste detta program få åtkomst till dina konton. Du kan hantera %1$ss tillstånd i Inställningar.\"</string><!--"To work properly, this app needs to access your accounts. You can manage %1$s’s permissions in Settings."-->

<!-- ******************************************************************************** Wizard Activity ******************************************************************************** -->

<!-- Title -->
    <string name="title_set_up_account">Skapa ett HTC-konto</string><!--Create an HTC Account-->

<!-- Email field hint. -->
    <string name="hint_email">E-postadress</string><!--Email address-->

<!-- Text tips for associated account  -->
    <string name="txt_other_accounts_tips">Ett HTC konto med din e-postadress har redan skapats. Tryck på ett konto för att logga in eller skapa ett nytt konto.</string><!--An HTC Account with your email address has already been created. Tap an account to sign in, or create a new account.-->

<!-- Subscribe news letter. -->
    <string name="txt_subscribe_newsletter">Skicka mig erbjudanden, produktinformation och nyhetsbrev från HTC.</string><!--Email me offers, product info and newsletters from HTC.-->

<!-- Text tips for the email retrieved from social account. The parameter %s will be replaced by a string. -->
    <string name="txt_social_email_tips">Använder e-post från %s-konto:</string><!--Using email from %s account:-->

<!-- Sign up button. -->
    <string name="btn_create_account">Skapa konto</string><!--Create account-->

<!-- Create new account button. -->
    <string name="btn_create_new_account">Skapa ett nytt konto</string><!--Create new account-->

<!-- Legal tips in create account page. The parameter %1$s will be replaced by the txt_sign_up_legal_tips_tos_link, and %2$s will be replaced by txt_sign_up_legal_tips_privacy_link. Example: "By creating an account, you agree to HTC Account's Terms and Conditions and acknowledge the Privacy Policy. You must be at least 14 years old to sign up." -->
    <string name="txt_sign_up_legal_tips">Genom att skapa ett konto accepterar du HTC kontots %1$s och godkänner %2$s. Du måste vara minst 14 år för att registrera dig.</string><!--By creating an account, you agree to HTC Account\'s %1$s and acknowledge the %2$s. You must be at least 14 years old to sign up.-->

<!-- Facebook legal tips. Terms and conditions hyper link. -->
    <string name="txt_sign_in_legal_tips_tos_link">Användarvillkor</string><!--Terms and Conditions-->

<!-- Facebook legal tips. Privacy hyper link. -->
    <string name="txt_sign_in_legal_tips_privacy_link">Sekretesspolicy</string><!--Privacy Policy-->

<!-- HTC's Privacy policy -->
    <string name="dialog_title_privacy_policy">Sekretesspolicy</string><!--Privacy policy-->

<!-- HTC terms and conditions dialog. Title. -->
    <string name="dialog_title_term_of_service">Användarvillkor</string><!--Terms and conditions-->

<!-- Email verification dialog. Title for dialog box -->
    <string name="dialog_title_email_verification">Verifiera ditt konto</string><!--Verify your account-->

<!-- Open mail button text -->
    <string name="dialog_btn_open_mail">Öppna e-postmeddelande</string><!--Open mail-->

<!-- Later button text. -->
    <string name="dialog_btn_later">Inte nu</string><!--Not now-->

<!-- Email verification dialog. Message for dialog box. The parameter %1$s will be replaced by email address, and %2$s will be replaced by dialog_link_resend. Example: "To sign in to HTC Account, you need to verify your email address using the link sent to email@domain.com.\n\nIf you didn't get the email, check your spam folder or resend it now." -->
    <string name="dialog_msg_email_verification">För att logga in på HTC konto måste du bekräfta din e-postadress genom den länk som skickades till %1$s.\n\nOm du inte fick e-postmeddelandet, kolla din skräppostmapp eller %2$s det nu.</string><!--To sign in to HTC Account, you need to verify your email address using the link sent to %1$s.\n\nIf you didn\'t get the email, check your spam folder or %2$s it now.-->

<!-- Email verification dialog. Resend link text. -->
    <string name="dialog_link_resend">skicka igen</string><!--resend-->

<!-- Toast to ask user to provide email address -->
    <string name="toast_txt_provide_email">E-postadress krävs.</string><!--Email address required.-->

<!-- Toast to tell user that the email is incorrect. -->
    <string name="toast_txt_error_email_incorrect">Ange en giltig e-postadress.</string><!--Please enter a valid email address.-->

<!-- Toast to show email address. For example, Email sent to: someone@domain.com -->
    <string name="toast_txt_email_sent_to_tip">E-post skickades till: %s</string><!--Email sent to: %s-->

