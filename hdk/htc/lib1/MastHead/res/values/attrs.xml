<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="MastheadView">
    <attr name="DisplayMode">
      <enum name="Normal" value="0" />
      <enum name="Simple" value="1" />
      <enum name="Large" value="2" />
      <enum name="Powersaving" value="3" />
      <enum name="Large2line" value="4" />
    </attr>
  </declare-styleable>  
  <declare-styleable name="MastheadMaskImageView">
 	<attr name="MastheadMask" format="reference" />
 	<attr name="MaskStyle">
 	    <enum name="Normal" value="0" />
      	<enum name="Upper" value="1" />
      	<enum name="Lower" value="2" />
 	</attr>
  </declare-styleable>    
</resources>
